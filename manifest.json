{
  "name" : "Snake v1.0.2",
  "version" : "1.0.2",
  "description" : "The snake game by Adrian Gjerstad in p5js.",
  "permissions" : ["declarativeContent", "storage"],
  "background" : {
    "scripts" : ["background.js"],
    "persistent" : false
  },
  "page_action" : {
    "default_popup" : "snake.html",
    "default_icon" : {
      "16"  : "icon16.png",
      "32"  : "icon32.png",
      "48"  : "icon48.png",
      "128" : "icon128.png"
    }
  },
  "manifest_version" : 2,
  "icons" : {
    "16"  : "icon16.png",
    "32"  : "icon32.png",
    "48"  : "icon48.png",
    "128" : "icon128.png"
  }
}
